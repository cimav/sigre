<div id="screen-name" class="header-catalogo">
  <h3>Clientes de Netmultix <span class="badge badge-muestras">{{this.length}}</span></h3>
</div>

<div id="md-general-panel" class="main-catalogo">

  <div id="md-items-list">
    <table class="table table-striped table-bordered">
      <tr>
        <th>Clave</th>
        <th>RFC</th>
        <th>Nombre</th>
      </tr>
      {{#each}}
        <tr>
          <td>  <a data-toggle="collapse"  {{bind-attr href=this.collapsable_href}}>{{cl01_clave}}</a>   </td>
          <td>{{cl01_rfc}}</td>
          <td>{{cl01_nombre}}</td>
        </tr>
        <tr>
          <td colspan="3">
            <div class="panel-group">
                <div {{bind-attr id=this.collapsable_id}} class="panel-collapse collapse">
                  <div class="panel-body">Panel Body</div>
                </div>
            </div>
          </td>
        </tr>
      {{/each}}
    </table>
  </div>
</div>

